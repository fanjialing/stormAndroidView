    <name>baofeng</name>
    <description>
        An Ionic Framework and Cordova project.
    </description>
    <content src="main.html" />
    <access origin="*" />   
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
           
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    
     <!-- 是否全屏显示 -->
    <preference name="Fullscreen" value="false" />
    <!-- 是否出现滚动条 -->
    <preference name="DisallowOverscroll" value="true" />
    <!-- 开启键盘上的工具 -->
    <preference name="HideKeyboardFormAccessoryBar" value="true"/>
    <!-- 设置屏幕显示方式  横屏或者竖屏 -->
    <platform name="android">
        <preference name="Orientation" value="sensorLandscape" />
    </platform>
    
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />  
    <preference name="SplashFirst" value="first" />  
    <preference name="SplashNoFirst" value="no_first" />  
    <preference name="LogInfo" value="true" /> 
    <preference name="LogDebug" value="true" /> 
    <preference name="LogError" value="true" /> 
    <preference name="GetCurrVersionUrl" value="http://192.168.0.15:8080/baofeng-app/version/getCurrVersion" /> 
    
        
    <feature name="Camera">
        <param name="android-package" value="com.laanto.it.app.camera.CameraLauncher" />
    </feature>
    <feature name="intbirdCallActivity">
        <param name="android-package" value="com.laanto.it.app.plugin.CallActivityPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="intbirdShowToast">
        <param name="android-package" value="com.laanto.it.app.plugin.ToastPlugin" />
    </feature>
  <feature name="nativeLogger">
        <param name="android-package" value="com.laanto.it.app.plugin.NativeLogger" />
    </feature>
    <feature name="BaiduPush">
        <param name="android-package" value="com.laanto.it.app.plugin.BaiduPushPlugin" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="com.laanto.it.app.plugin.SplashScreen" />
        <param name="onload" value="false" />
    </feature>
      <feature name="Whitelist">
        <param name="android-package" value="com.laanto.it.app.plugin.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
      <feature name="Device">
        <param name="android-package" value="com.laanto.it.app.plugin.Device" />
        <param name="onload" value="true" />
    </feature>
      <feature name="RetreatActivity">
        <param name="android-package" value="com.laanto.it.app.plugin.RetreatActivityPlugin" />
        <param name="onload" value="true" />
    </feature>
    
     <feature name="NetworkSetting">
        <param name="android-package" value="com.laanto.it.app.plugin.NetworkSettingPlugin" />
        <param name="onload" value="true" />
    </feature>
</widget>
