<?xml version="1.0" encoding="utf-8"?>
<resources>  
    <declare-styleable name="circleloader">
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textFakeBold" format="boolean" />
        <attr name="textColor" format="reference|color" />
        <attr name="circleRadius" format="dimension" />
        <attr name="circleStrokeWidth" format="dimension" />
        <attr name="circleColor" format="reference|color" />
        <attr name="circleAndTextSpacing" format="dimension" />
        <attr name="backgroundColor" format="reference|color" />
    </declare-styleable>
    
    
        <declare-styleable name="Rotate3dAnimation">
        <attr name="rollType" format="enum">
            <enum name="x" value="0"/>
            <enum name="y" value="1"/>
            <enum name="z" value="2"/>
        </attr>
        <attr name="fromDeg" format="float" />
        <attr name="toDeg" format="float" />
        <attr name="pivotX" format="fraction"/>
        <attr name="pivotY" format="fraction" />
    </declare-styleable>
</resources>
